{
  "Ocr": {
    "DefaultMode": "AUTO",
    "Tesseract": {
      "TessdataPath": "models/tessdata",
      "Languages": "vie+eng",
      "Psm": 6,
      "Oem": 1,
      "Whitelist": ""
    },
    "Onnx": {
      "DetModel": "models/onnx/ppocrv3_det.onnx",
      "RecModel": "models/onnx/ppocrv3_rec.onnx",
      "Provider": "CPU",
      "UseGpu": false,
      "ThreadCount": 4
    }
  },
  "ConnectionStrings": {
    "Default": "Data Source=./data/ocr.sqlite"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
